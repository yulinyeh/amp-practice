{
  "name": "amp-nuxt",
  "version": "1.0.0",
  "description": "My beautiful Nuxt.js project",
  "author": "Yulin Yeh",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "generate": "nuxt-ts generate",
    "start": "nuxt-ts start",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,vue}": "npm run lint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "0.4.2",
    "@nuxtjs/amp": "0.2.4",
    "@nuxtjs/axios": "5.9.7",
    "@nuxtjs/dotenv": "1.4.1",
    "@nuxtjs/pwa": "3.0.0-beta.20",
    "nuxt": "2.12.2",
    "nuxt-i18n": "6.8.1"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "0.6.2",
    "@nuxtjs/eslint-config-typescript": "1.0.2",
    "@nuxtjs/eslint-module": "1.1.0",
    "@nuxtjs/stylelint-module": "3.2.2",
    "@nuxtjs/tailwindcss": "1.4.0",
    "@vue/test-utils": "1.0.0-beta.33",
    "babel-eslint": "10.1.0",
    "babel-jest": "25.3.0",
    "eslint": "6.8.0",
    "eslint-plugin-nuxt": "0.5.2",
    "husky": "4.2.3",
    "jest": "25.3.0",
    "lint-staged": "10.1.2",
    "pug": "3.0.1",
    "pug-plain-loader": "1.0.0",
    "stylelint": "13.3.0",
    "ts-jest": "25.3.1",
    "vue-jest": "4.0.0-beta.2"
  }
}
